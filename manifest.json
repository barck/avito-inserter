{
	"manifest_version": 2,
	"name": "Avito inserter",
	"description": "Подстановка значений в поля",
	"version": "1.0.0",
	"icons": {"128": "icon.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
		  "matches": [
			"<all_urls>"
		  ],
		  "js": ["app.js"]
		}
	],
	"permissions": ["activeTab"]
}